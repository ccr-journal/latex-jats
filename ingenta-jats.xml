<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE article PUBLIC "-//NLM//DTD JATS (Z39.96) Journal Publishing DTD v1.1 20151215//EN" "https://jats.nlm.nih.gov/publishing/1.1/JATS-journalpublishing1.dtd">
<article article-type="research-article">
    <front>
        <journal-meta>
            <journal-id journal-id-type="publisher-id">CCR</journal-id>
            <journal-title-group>
                <journal-title>Computational Communication Research</journal-title>
            </journal-title-group>
            <issn pub-type="print">2665-9085</issn>
            <issn pub-type="electronic">2665-9085</issn>
            <publisher>
                <publisher-name>Amsterdam University Press</publisher-name>
                <publisher-loc>Amsterdam</publisher-loc>
            </publisher>
        </journal-meta>
        <article-meta>
            <article-id pub-id-type="publisher-id">$article.publisher_id$</article-id>
            <article-id pub-id-type="doi">$article.doi$</article-id>
            <article-categories>
                <subj-group>
                    <subject>Article</subject>
                </subj-group>
            </article-categories>
            <title-group>
                <article-title>$title$</article-title>
            </title-group>
            <contrib-group>
$for(author)$
                <contrib contrib-type="author">
                    <name>
                        <string-name>$author.surname$</string-name>
                    </name>
                    <aff>$author.affiliation.name$</aff>
                </contrib>
$endfor$
            </contrib-group>
            <pub-date pub-type="electronic">
                <year>$date.year$</year>
            </pub-date>
            <volume>$article.volume$</volume>
            <issue>$article.pubnumber$</issue>
            <permissions>
                <copyright-statement>$copyright.statement$</copyright-statement>
                <copyright-year>$copyright.year$</copyright-year>
                <copyright-holder>$copyright.holder$</copyright-holder>
                <license license-type="open-access" xlink:href="http://creativecommons.org/licenses/by/4.0/">
                </permissions>
                <abstract>
                    <p content-type="AUP_AL_NoInd">$abstract$</p>
                </abstract>
                <kwd-group>
                    <title>Keywords:</title>
$for(tags)$
                    <kwd>$tags$</kwd>
$endfor$
                </kwd-group>
            </article-meta>
        </front>
        <body>
        $body$
        </body>
    </article>
